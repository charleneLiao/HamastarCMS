	<div class="in">
		<div class="ct">
			<div class="in">
				<!-- link 連結 -->
				<%= render partial: "/erb/list-text_link", locals: set({ index: 1, header_text: "上方連結" }) %>
				<!-- link 結束 -->

				<!-- nav 選單 -->
				<%= render partial: "/erb/group-list_nav", locals: set({ index: 2, func_set: "{'majorNavHorizontal':{},'toggleBtn':{'cookie':'true'},'bindNode':'h3 a'}", header_text: "主選單", inner: "/erb/list-text_nav", menuType: "Menu" }) %>
